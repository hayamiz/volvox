
<article class="entry">
  <header class="entry-header">
    <h1><%= @entry.date.to_s %></h1>
    <p>Updated at: <%= @entry.updated_at %></p>
    <p>Temperature: <%= @entry.temperature %>&#x2103;, Humidity: <%= @entry.humidity %>% </p>
  </header>
  <section class="entry-body">
    <%= @entry.memo %>
  </section>
  <footer>
    <nav>
      <%= link_to "Back to diary", diary_path(@diary) %>
    </nav>
  </footer>
</article>
